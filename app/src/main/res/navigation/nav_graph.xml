            android:id="@+id/action_sentence_list_to_quiz"
            app:destination="@id/quizFragment" />
        <action
            android:id="@+id/action_sentence_list_to_writing_practice"
            app:destination="@id/writingPracticeFragment" />
            <action
            android:id="@+id/action_sentence_list_to_weak_sentence_mode"
            app:destination="@id/quizWeakSentenceModeFragment" />
    </fragment>


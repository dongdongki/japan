            app:destination="@id/songListFragment" />
        <action
            android:id="@+id/action_song_selection_to_song_lyrics"
            app:destination="@id/songLyricsFragment" />
    </fragment>

    <!-- Song Lyrics Fragment -->
    <fragment
        android:id="@+id/songLyricsFragment"
        android:name="com.example.myapplication.ui.SongLyricsFragment"
        android:label="">

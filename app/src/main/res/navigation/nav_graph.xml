        android:id="@+id/sentenceListFragment"
        android:name="com.example.myapplication.ui.SentenceListFragment"
        android:label="문장 목록"
        tools:layout="@layout/fragment_sentence_list">
        <argument
            android:name="showOnlyWeakSentences"
            app:argType="boolean"
            android:defaultValue="false" />
        <argument
            android:name="batchId"
            app:argType="string"
            app:nullable="true"

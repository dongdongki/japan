            android:name="wordId"
            app:argType="integer"
            android:defaultValue="-1" />
        <argument
            android:name="songId"
            app:argType="integer"
            android:defaultValue="-1" />
        <argument
            android:name="isSong"
            app:argType="boolean"
            android:defaultValue="false" />
        <argument
            android:name="sentenceId"
            app:argType="integer"
            android:defaultValue="-1" />
        <argument
            android:name="isSentence"
            app:argType="boolean"
            android:defaultValue="false" />
        <argument
            android:name="isWeakWords"
            app:argType="boolean"
            android:defaultValue="false" />
        <argument
            android:name="partOfSpeech"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="batchId"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
